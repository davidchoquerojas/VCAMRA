*
{
    padding:0;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: auto;
}
body
{
   font-family:Calibri;
}
/*estilos para jtable*/
section .jtable
{
    font-size:11px;
}
select, input[type="text"]
{
    font-size:12px;
}
header,nav,section
{
    margin-left:2%;
    margin-right:2%;
}
section
{
    min-height:80%;
    height:auto!important;
}
header #titulo .lbl_header
{
    float:left;
    clear:left;
}
header #titulo #ctl00_lbl_title,#ctl00_lbl_titulo
{
    color:#797979;
    font-size:35px;
    font-weight:bold;
}
header #titulo #lbl_user, #lbl_connect
{
    color:#797979;
    font-weight:bold;
    font-size: 12px;
}
header #titulo span
{
    color:#797979;
    float:left;
    margin-left:1%;
}

header #img_titulo
{
    margin-left:78%;
    width:200px;
}
header #img_titulo img
{
    height:100%;
    width:100%;
}
nav
{
    margin-bottom:0.4%;
}
section .siteMap
{
    border-bottom:1px dashed #797979;
    clear:left;
    margin-bottom:0.5%;
    float:left;
    font-weight:bold;
    line-height:3;
    color:#797979;
    width:50%;
}

