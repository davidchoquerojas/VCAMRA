/*Estilo para tablas editables de PRIMAS Y IBNR*/
#ctl00_ContentPlaceHolder1_gvPrima,#ctl00_ContentPlaceHolder1_gvIbnr{
    border:1px solid #ddd;
    font-family:'Segoe UI Semilight','Open Sans',Verdana,Arial,Helvetica,sans-serif;
    font-size:11px;
    float:left;
    position:relative;
    text-align:center;
    right:8%;
    width:80%;
}
#ctl00_ContentPlaceHolder1_gvPrima tr:nth-child(even),#ctl00_ContentPlaceHolder1_gvIbnr tr:nth-child(even)  {background-color: #f9f9f9;}
#ctl00_ContentPlaceHolder1_gvPrima tr:nth-child(odd),#ctl00_ContentPlaceHolder1_gvIbnr tr:nth-child(odd){background-color:#fff;}

#ctl00_ContentPlaceHolder1_gvPrima td, #ctl00_ContentPlaceHolder1_gvIbnr td{padding: 2px 2px;}
#ctl00_ContentPlaceHolder1_gvPrima tr td:nth-child(3),#ctl00_ContentPlaceHolder1_gvPrima tr th:nth-child(3),
#ctl00_ContentPlaceHolder1_gvIbnr tr td:nth-child(3),#ctl00_ContentPlaceHolder1_gvIbnr tr th:nth-child(3){
    display:none;
}
#ctl00_ContentPlaceHolder1_gvPrima caption, #ctl00_ContentPlaceHolder1_gvIbnr caption {
   border:1px solid #ddd;font-size:1.3em;font-family:'Segoe UI Semilight','Open Sans',Verdana,Arial,Helvetica,sans-serif;color:#009BB8;font-weight:bold;padding-top:10px;
}
#ctl00_ContentPlaceHolder1_gvPrima tr:hover,#ctl00_ContentPlaceHolder1_gvIbnr tr:hover {
    background:rgba(0,0,0,.1);
    cursor:pointer;
}
#ctl00_ContentPlaceHolder1_gvPrima tr td input[type="text"],#ctl00_ContentPlaceHolder1_gvIbnr tr td input[type="text"] {
    font-size:0.8em;
    height:25px;
    width:80%;
}
