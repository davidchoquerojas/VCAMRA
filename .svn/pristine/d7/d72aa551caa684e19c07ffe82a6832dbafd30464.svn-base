/*css popput*/
section .divOpacity
{
    visibility:hidden;
    position: absolute;
    top: 0%;
    left: 0%;
    width: 100%;
    height: 100%;
    background:rgba(0,0,0,.2);
    filter: alpha(opacity=80);
}
section .gridViewPop
{
    position: absolute;
    top: 20%;
    left: 9%;
    width: 80%;
    height: 65%;
    padding: 0px 16px 16px 16px;
    border: 5px solid #009bb8;
    background-color: white;
    z-index:1002;
    overflow: auto;
}
section  .close
{
    background:#009bb8;
    border-radius:50%;
    float:right;
    font-size:25px;
    font-weight:bold;
    color:black;
    padding:0.7%;
    position:relative;
    right:8%;
    top:13.4%;
}
