    <!--<add key="CnnBD" value="Data Source=150.100.10.30;Initial Catalog=VCamara;user id=APACAYA;pwd=123" />-->
    <add key="CnnBD" value="Data Source=.;Initial Catalog=vcamara;Integrated Security=True"  />
